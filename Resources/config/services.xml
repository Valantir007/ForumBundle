<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="breadcrumb_service" class="Valantir\ForumBundle\Service\Breadcrumb">
            <argument type="service" id="service_container"/>
        </service>

        <service id="bb_code_extension_parser" class="Valantir\ForumBundle\Service\BBCodeExtensionParser">
            <argument type="service" id="service_container"/>
        </service>

        <service id="bb_code_golonka_parser" class="Valantir\ForumBundle\Service\BBCodeGolonkaParser">
            <argument type="service" id="service_container"/>
        </service>
    </services>

</container>
